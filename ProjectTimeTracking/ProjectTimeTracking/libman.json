{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@5.13.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "sweetalert@2.1.2",
      "destination": "wwwroot/lib/sweetalert/"
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/",
      "files": [
        "toastr.min.js",
        "toastr.min.css"
      ]
    },
    {
      "library": "datatables@1.10.20",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "js/jquery.dataTables.min.js",
        "images/sort_asc.png",
        "images/sort_asc_disabled.png",
        "images/sort_both.png",
        "images/sort_desc.png",
        "images/sort_desc_disabled.png",
        "css/jquery.dataTables.min.css"
      ]
    },
    {
      "library": "moment.js@2.25.3",
      "destination": "wwwroot/lib/moment.js/",
      "files": [
        "moment.min.js"
      ]
    }
  ]
}